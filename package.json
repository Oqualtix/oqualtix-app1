{
  "name": "oqualtix-app",
  "version": "1.0.0",
  "description": "AI-Powered Financial Forensics Platform for Embezzlement Detection",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build": "expo build",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "validate": "node scripts/validate-build.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "expo": "~49.0.0",
    "expo-status-bar": "~1.6.0",
    "expo-document-picker": "~11.5.4",
    "expo-linear-gradient": "~12.3.0",
    "expo-crypto": "~12.4.1",
    "expo-secure-store": "~12.3.1",
    "expo-local-authentication": "~13.4.1",
    "expo-notifications": "~0.20.1",
    "react": "18.2.0",
    "react-native": "0.72.17",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^5.10.4",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-csv": "^0.2.0",
    "react-native-fs": "^2.20.0",
    "crypto-js": "^4.1.1",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "openai": "^4.15.0",
    "axios": "^1.5.0",
    "ml-matrix": "^6.10.4",
    "ml-regression": "^6.3.0",
    "natural": "^6.5.0",
    "compromise": "^14.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "eslint": "^8.50.0",
    "eslint-config-expo": "^7.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~49.0.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "testMatch": [
      "**/__tests__/**/*.(js|jsx|ts|tsx)",
      "**/*.(test|spec).(js|jsx|ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "eslintConfig": {
    "extends": "expo",
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "keywords": [
    "financial-forensics",
    "embezzlement-detection",
    "ai-powered",
    "fraud-detection",
    "react-native",
    "expo",
    "forensic-accounting"
  ],
  "author": "Oqualtix Forensics",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Oqualtix/oqualtix-app1.git"
  },
  "bugs": {
    "url": "https://github.com/Oqualtix/oqualtix-app1/issues"
  },
  "homepage": "https://github.com/Oqualtix/oqualtix-app1#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "private": true
}