{
  "name": "oqualtix-app",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-Powered Financial Forensics Platform for Embezzlement Detection",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "fraud-detection": "node secure_fraud_detection_cli.js",
    "admin": "node admin_license_manager.js",
    "auth-test": "node quick_auth_test.js",
    "auth-demo": "node authentication_demo.js",
    "eject": "expo eject",
    "build": "expo build",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "validate": "node scripts/validate-build.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/slider": "^5.1.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "axios": "^1.5.0",
    "compromise": "^14.10.0",
    "crypto-js": "^4.1.1",
    "expo": "^54.0.20",
    "expo-crypto": "~15.0.7",
    "expo-document-picker": "~14.0.7",
    "expo-font": "^14.0.9",
    "expo-linear-gradient": "~15.0.7",
    "expo-local-authentication": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-notifications": "^0.32.12",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-status-bar": "~3.0.8",
    "joi": "^17.11.0",
    "ml-matrix": "^6.10.4",
    "ml-regression": "^6.3.0",
    "moment": "^2.29.4",
    "natural": "^6.5.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-csv": "^0.2.0",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.10.4",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.14.0",
    "react-native-worklets": "^0.6.1",
    "validator": "^13.15.20"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "eslint": "^8.50.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~49.0.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "testMatch": [
      "**/__tests__/**/*.(js|jsx|ts|tsx)",
      "**/*.(test|spec).(js|jsx|ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "eslintConfig": {
    "extends": "expo",
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "keywords": [
    "financial-forensics",
    "embezzlement-detection",
    "ai-powered",
    "fraud-detection",
    "react-native",
    "expo",
    "forensic-accounting"
  ],
  "author": {
    "name": "Oqualtix Forensics",
    "email": "Oqualtix@outlook.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Oqualtix/oqualtix-app1.git"
  },
  "bugs": {
    "url": "https://github.com/Oqualtix/oqualtix-app1/issues"
  },
  "homepage": "https://github.com/Oqualtix/oqualtix-app1#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "private": true
}
